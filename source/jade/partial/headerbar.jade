header
    h1 {{ 'BIOGRAPHY.NAME' | translate | words: 2}}
    nav#navigation-bar
        ul
            li
                a(href="/") {{ 'FIELDS.MAINBAR.SKILLS' | translate }}
            li
                a(href="/solucoes") {{ 'FIELDS.MAINBAR.SOLUTIONS' | translate }}
            li
                a(href="/projetos") {{ 'FIELDS.MAINBAR.PROJECTS' | translate }}
