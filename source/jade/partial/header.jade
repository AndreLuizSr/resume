header
    h1 {{ 'BIOGRAPHY.NAME' | translate | words: 2}}
    nav#navigation-bar
        ul
            li
                a(href="/")
                    i.fa.fa-code
                    | {{ 'FIELDS.MAINBAR.SKILLS' | translate }}
            li
                a(href="/solucoes")
                    i.fa.fa-code 
                    | {{ 'FIELDS.MAINBAR.SOLUTIONS' | translate }}
            li
                a(href="/projetos") 
                    i.fa.fa-code
                    | {{ 'FIELDS.MAINBAR.PROJECTS' | translate }}
            li
                a(href="/contato")
                    i.fa.fa-envelope-o
                    | {{ 'FIELDS.MAINBAR.CONTACT' | translate }}
            li
                select(ng-options='lang.language for lang in avaliableLanguages track by lang.i18n', ng-model='selectedLanguage', ng-change='changeLanguage()', class="form-control")
